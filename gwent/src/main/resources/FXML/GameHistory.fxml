<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.text.Text?>
<AnchorPane prefHeight="450.0" prefWidth="780.0" xmlns="http://javafx.com/javafx/17.0.2-ea"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.GameHistoryController">
    <children>
        <ImageView cache="true" fitHeight="466.0" fitWidth="788.0" layoutX="-8.0">
            <image>
                <Image url="@../images/MenusBackground.jpg"/>
            </image>
            <cursor>
                <Cursor fx:constant="DEFAULT"/>
            </cursor>
        </ImageView>

        <VBox alignment="CENTER" layoutX="140.0" layoutY="10.0" prefHeight="360.0" prefWidth="500.0" spacing="10.0">
            <children>

                <Label contentDisplay="CENTER" layoutX="319.0" layoutY="68.0" text="Game History" textFill="WHITE">
                    <font>
                        <Font name="Copperplate Gothic Bold" size="22.0"/>
                    </font>
                </Label>

                <HBox alignment="CENTER" spacing="10">
                    <TextField fx:id="number" promptText="how many game history do you want to see?" prefWidth="400"/>
                    <Button mnemonicParsing="false" text="submit" onMouseClicked="#setText" />
                </HBox>
                <Text fx:id="t" />

                <Button mnemonicParsing="false" prefWidth="450" text="Back" onMouseClicked="#backFromGameHistory"/>


            </children>
        </VBox>
    </children>
</AnchorPane>
